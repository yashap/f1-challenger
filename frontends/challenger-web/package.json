{
  "name": "@f1-challenger/challenger-web",
  "version": "0.0.1",
  "private": true,
  "source": "src/index.html",
  "scripts": {
    "build": "parcel build --no-source-maps",
    "clean": "rm -rf node_modules/ dist/ coverage/ .turbo/ .parcel-cache/ ../../.parcel-cache/",
    "format": "eslint \"src/**\" --fix",
    "lint": "tsc --noEmit && eslint \"src/**\"",
    "serve": "parcel --open",
    "test": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest --forceExit --runInBand --passWithNoTests src/",
    "test:watch": "LOG_LEVEL=\"${LOG_LEVEL:-off}\" jest --forceExit --runInBand  --passWithNoTests --watch src/"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@f1-challenger/api-client-utils": "*",
    "@f1-challenger/challenger-client": "*",
    "@f1-challenger/errors": "*",
    "@fontsource/roboto": "^5.1.1",
    "@js-temporal/polyfill": "^0.4.4",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@mui/x-data-grid": "^7.23.6",
    "@tanstack/react-query": "^5.62.15",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.1",
    "supertokens-auth-react": "^0.48.0",
    "supertokens-web-js": "^0.14.0"
  },
  "devDependencies": {
    "@f1-challenger/eslint-config": "*",
    "@f1-challenger/test-utils": "*",
    "@f1-challenger/tsconfig": "*",
    "@tanstack/eslint-plugin-query": "^5.62.15",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "jest": "^29.7.0",
    "parcel": "^2.13.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "typescript": "~5.5.4"
  }
}
